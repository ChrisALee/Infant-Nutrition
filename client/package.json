{
    "name": "infant-nutrition-client",
    "version": "1.1.0",
    "author": "Christopher Lee",
    "private": true,
    "scripts": {
        "dev": "next",
        "build": "next build",
        "postinstall": "next build",
        "start": "next start -p 8080",
        "analyze": "cross-env ANALYZE=1 next build",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
        "prettier": "prettier  --write **/*.ts*",
        "format": "npm run prettier && npm run tslint",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.{ts,tsx}": ["tslint --fix", "prettier --write", "git add"]
    },
    "files": [
        ".babelrc",
        "components",
        "next.config.js",
        "package-lock.json",
        "package.json",
        "pages",
        "static",
        "store.ts",
        "tsconfig.json",
        "utils"
    ],
    "dependencies": {
        "@material-ui/icons": "^1.0.0-beta.42",
        "@types/next": "^2.4.9",
        "@types/next-redux-wrapper": "^1.3.7",
        "@types/react": "^16.3.10",
        "@zeit/next-typescript": "0.1.1",
        "babel-plugin-styled-components": "^1.5.1",
        "dotenv": "^5.0.1",
        "fork-ts-checker-webpack-plugin": "^0.4.1",
        "isomorphic-unfetch": "^2.0.0",
        "jss": "^9.8.1",
        "jwt-decode": "^2.2.0",
        "material-ui": "^1.0.0-beta.41",
        "next": "^6.0.0-canary.3",
        "next-redux-wrapper": "^1.3.5",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-jss": "^8.4.0",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-state-sync": "^1.1.0",
        "redux-thunk": "^2.2.0",
        "styled-components": "^3.2.5",
        "typescript": "^2.8.1",
        "webpack-bundle-analyzer": "^2.11.1"
    },
    "devDependencies": {
        "cross-env": "^5.1.4",
        "lint-staged": "^7.0.4",
        "prettier": "^1.12.0",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-eslint-rules": "^5.1.0",
        "tslint-react": "^3.5.1"
    }
}

        "styled-components": "^3.1.0-3",
        "typescript": "^2.8.1"
    },
    "devDependencies": {
        "ajv": "^6.4.0",
        "cross-env": "^5.1.4",
        "lint-staged": "^7.0.1",
        "nodemon": "^1.17.3",
        "prettier": "^1.11.1",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-eslint-rules": "^5.1.0",
        "tslint-react": "^3.5.1"
    }

{
    "name": "infant-nutrition-client",
    "version": "1.1.0",
    "author": "Christopher Lee",
    "private": true,
    "scripts": {
        "dev": "next",
        "build": "next build",
        "prestart": "npm run build",
        "start": "set NODE_ENV=production&& next start -p 8080",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
        "prettier": "prettier  --write **/*.ts*",
        "format": "npm run prettier && npm run tslint",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "tslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "@material-ui/icons": "^1.0.0-beta.42",
        "@types/next": "^2.4.8",
        "@types/next-redux-wrapper": "^1.3.7",
        "@types/react": "^16.3.6",
        "@types/styled-jsx": "^2.2.2",
        "@zeit/next-typescript": "0.1.1",
        "babel-plugin-styled-components": "^1.5.1",
        "dotenv": "^5.0.1",
        "draft-js": "^0.10.5",
        "immutable": "^3.8.2",
        "isomorphic-unfetch": "^2.0.0",
        "jss": "^9.8.1",
        "jwt-decode": "^2.2.0",
        "material-ui": "^1.0.0-beta.41",
        "next": "^6.0.0-canary.3",
        "next-redux-wrapper": "^1.3.5",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-draft-wysiwyg": "^1.12.11",
        "react-jss": "^8.4.0",
        "react-redux": "^5.0.7",
        "react-tap-event-plugin": "^3.0.2",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-state-sync": "^1.1.0",
        "redux-thunk": "^2.2.0",
        "styled-components": "^3.2.5",
        "typescript": "^2.8.1"
    },
    "devDependencies": {
        "ajv": "^6.4.0",
        "cross-env": "^5.1.4",
        "lint-staged": "^7.0.4",
        "prettier": "^1.11.1",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-eslint-rules": "^5.1.0",
        "tslint-react": "^3.5.1"
    }
}

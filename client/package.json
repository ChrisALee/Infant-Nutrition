        "@zeit/next-typescript": "0.0.10",
        "babel-plugin-styled-components": "^1.5.1",
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "isomorphic-unfetch": "^2.0.0",
        "jss": "^9.8.1",
        "jwt-decode": "^2.2.0",
        "material-ui": "^1.0.0-beta.39",
        "material-ui-icons": "^1.0.0-beta.36",
        "next": "^5.1.0",
        "next-redux-wrapper": "^1.3.5",
        "react": "^16.2.0",

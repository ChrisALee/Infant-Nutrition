    "author": "Christopher Lee",
    "private": true,
    "scripts": {
        "dev": "next",
        "build": "next build",
        "prestart": "npm run build",
        "start": "next start -p 8080",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
        "prettier": "prettier  --write **/*.ts*",
